<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PersonalTrainer"
        tools:targetApi="31">
        <activity
            android:name=".view.Rutina.ListaRutinasActivity"
            android:exported="false" />
        <activity
            android:name=".view.Rutina.GestionarRutinaActivity"
            android:exported="false" />
        <activity
            android:name=".view.Ejercicio.GestionarEjercicioActivity"
            android:exported="false" />
        <activity
            android:name=".view.Ejercicio.ListaEjercicioActivity"
            android:exported="false" />
        <activity
            android:name=".view.Categoria.GestionarCategoriaActivity"
            android:exported="false" />
        <activity
            android:name=".view.Categoria.ListaCategoriaActivity"
            android:exported="false" />
        <activity
            android:name=".view.Cliente.GestionarClienteActivity"
            android:exported="false" />
        <activity
            android:name=".view.Cliente.ListaClienteActivity"
            android:exported="false" />
        <activity
            android:name=".view.Objetivo.AgregarObjetivoActivity"
            android:exported="false" />
        <activity
            android:name=".view.Objetivo.ListObjetivoActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>